package javaclassweek5;
public class Employee 
{
    private int empNum;
    private String empFirstName;
    private String empLastName;
    private double empSalary;
    
    //constructor //constructor has to be public
    public Employee(int n, String l, String f, double s) //no void... same name as class
    {
        empNum = n;
        empLastName = l;
        empFirstName = f;
        empSalary = s;
    }
    
    //getters and setters:
    public int getEmpNum()
    { return empNum; }
    public void setEmpNum(int emp)
    { empNum = emp; }
    public String getEmpLastName()
    { return empLastName; }
    public void setEmpLastName(String name)
    { empLastName = name;}
    public String getEmpFirstName()
    { return empFirstName;}
    public void setEmpFirstName(String name)
    { empFirstName = name; } 
    public double getEmpSalary()
    { return empSalary; }
    public void setEmpSalary (double sal)
    { empSalary = sal; }
}
